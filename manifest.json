{
    "manifest_version": 3,
    "name": "Untrapped",
    "version": "0.0.2",
    "content_scripts": [
        {
            "matches": [
                "https://*.youtube.com/*"
            ],
            
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "untrapped.svg"
    }
}