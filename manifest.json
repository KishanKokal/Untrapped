{
    "manifest_version": 3,
    "name": "Untrapped",
    "version": "0.0.2",
    "permissions": ["storage", "tabs"],
    "host_permissions": ["https://*.youtube.com/*"],
    "content_scripts": [
        {
            "matches": [
                "https://*.youtube.com/*"
            ],
            
            "js": [
                "content.js"
            ],

            "run_at": "document_end"
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "untrapped.png",
        "default_popup": "popup.html"
    }
}